<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Windore.Settings.GUI.DefaultSettingControl">
  <Grid ColumnDefinitions="300,280,250" HorizontalAlignment="Left" Margin="5">
    <TextBlock Name="settingNameTB" VerticalAlignment="Center" Grid.Column="0"></TextBlock>
    <TextBox AcceptsReturn="False" AcceptsTab="False" Name="settingInputTB" Height="28" Grid.Column="1"></TextBox>
    <TextBlock Margin="5,0,0,0" Name="invalidTB" Foreground="Red" VerticalAlignment="Center" Grid.Column="2"></TextBlock>
  </Grid>
</UserControl>
